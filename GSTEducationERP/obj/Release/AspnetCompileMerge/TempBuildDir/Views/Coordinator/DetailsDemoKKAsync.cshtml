@model GSTEducationERPLibrary.Coordinator.Coordinator

@using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.ScheduleId)
@*<div class="card" style="width: 400px; margin: 0 auto;">*@
   <style>
       h2 {
           color: #64bbc9;
           font-weight: bold;
           font-size: 30px;
           margin-left: 17px;
           margin-bottom: -10px;
           font-family: sans-serif;
           text-align: center;
       }
   </style>
        <div class="container-fluid">
        <br />

        <div class="card mx-auto col-md-10">
            <div class="form-horizontal">
                <button class="close text-danger" data-dismiss="modal" style="position: absolute; top: 8px; right: 8px;">&times;</button>
                <h2>Demo Details</h2>
                <hr />
              <div class="row">
                <div class="col-md-6">
                    <label>Batch Code:</label>
                    <span>@Model.BatchCode</span>
                </div>
                <div class="col-md-6">
                    <label>Demo Name:</label>
                    <span>@Model.DemoName</span>
                </div>
                <div class="col-md-6">
                    <label>Trainer Name:</label>
                    <span>@Model.TrainerName</span>
                </div>
                <div class="col-md-6">
                    <label>Demo ArrangedBy:</label>
                    <span>@Model.DemoArrangedBy</span>
                </div>
                <div class="col-md-6">
                    <label>Lab Name:</label>
                    <span>@Model.LabName</span>
                </div>
                <div class="col-md-6">
                    <label>NoOfStudent:</label>
                    <span>@Model.NoOfStudent</span>
                </div>
                <div class="col-md-6">
                    <label>Date:</label>
                    <span>@Model.ExpecDate</span>
                </div>
                <div class="col-md-6">
                    <label>Status:</label>
                    <span>@Model.StatusName</span>
                </div>
            </div>
            </div>
    </div>
    </div>
}



